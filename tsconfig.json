{
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  },
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./",
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "incremental": true,
    "importHelpers": true,
    "lib": [
      "es2018",
      "dom"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "outDir": "./dist/out-tsc",
    "paths": {
      "@terminus/ui": [
        "dist/library/terminus-ui",
        "dist/library"
      ],
      "@terminus/ui/autocomplete":        ["projects/library/autocomplete/src/public-api.ts"],
      "@terminus/ui/autofocus":           ["projects/library/autofocus/src/public-api.ts"],
      "@terminus/ui/button":              ["projects/library/button/src/public-api.ts"],
      "@terminus/ui/card":                ["projects/library/card/src/public-api.ts"],
      "@terminus/ui/chart":               ["projects/library/chart/src/public-api.ts"],
      "@terminus/ui/checkbox":            ["projects/library/checkbox/src/public-api.ts"],
      "@terminus/ui/chip":                ["projects/library/chip/src/public-api.ts"],
      "@terminus/ui/cohort-date-range":   ["projects/library/cohort-date-range/src/public-api.ts"],
      "@terminus/ui/confirmation":        ["projects/library/confirmation/src/public-api.ts"],
      "@terminus/ui/copy":                ["projects/library/copy/src/public-api.ts"],
      "@terminus/ui/csv-entry":           ["projects/library/csv-entry/src/public-api.ts"],
      "@terminus/ui/date-range":          ["projects/library/date-range/src/public-api.ts"],
      "@terminus/ui/drawer":              ["projects/library/drawer/src/public-api.ts"],
      "@terminus/ui/expansion-panel":     ["projects/library/expansion-panel/src/public-api.ts"],
      "@terminus/ui/file-upload":         ["projects/library/file-upload/src/public-api.ts"],
      "@terminus/ui/form-field":          ["projects/library/form-field/src/public-api.ts"],
      "@terminus/ui/icon":                ["projects/library/icon/src/public-api.ts"],
      "@terminus/ui/icon-button":         ["projects/library/icon-button/src/public-api.ts"],
      "@terminus/ui/input":               ["projects/library/input/src/public-api.ts"],
      "@terminus/ui/link":                ["projects/library/link/src/public-api.ts"],
      "@terminus/ui/loading-overlay":     ["projects/library/loading-overlay/src/public-api.ts"],
      "@terminus/ui/login-form":          ["projects/library/login-form/src/public-api.ts"],
      "@terminus/ui/logo":                ["projects/library/logo/src/public-api.ts"],
      "@terminus/ui/menu":                ["projects/library/menu/src/public-api.ts"],
      "@terminus/ui/navigation":          ["projects/library/navigation/src/public-api.ts"],
      "@terminus/ui/option":              ["projects/library/option/src/public-api.ts"],
      "@terminus/ui/paginator":           ["projects/library/paginator/src/public-api.ts"],
      "@terminus/ui/pipes":               ["projects/library/pipes/src/public-api.ts"],
      "@terminus/ui/popover":             ["projects/library/popover/src/public-api.ts"],
      "@terminus/ui/radio-group":         ["projects/library/radio-group/src/public-api.ts"],
      "@terminus/ui/scrollbars":          ["projects/library/scrollbars/src/public-api.ts"],
      "@terminus/ui/search":              ["projects/library/search/src/public-api.ts"],
      "@terminus/ui/select":              ["projects/library/select/src/public-api.ts"],
      "@terminus/ui/selection-list":      ["projects/library/selection-list/src/public-api.ts"],
      "@terminus/ui/sort":                ["projects/library/sort/src/public-api.ts"],
      "@terminus/ui/spacing":             ["projects/library/spacing/src/public-api.ts"],
      "@terminus/ui/table":               ["projects/library/table/src/public-api.ts"],
      "@terminus/ui/tabs":                ["projects/library/tabs/src/public-api.ts"],
      "@terminus/ui/toggle":              ["projects/library/toggle/src/public-api.ts"],
      "@terminus/ui/tooltip":             ["projects/library/tooltip/src/public-api.ts"],
      "@terminus/ui/utilities":           ["projects/library/utilities/src/public-api.ts"],
      "@terminus/ui/validation-messages": ["projects/library/validation-messages/src/public-api.ts"],
      "@terminus/ui/validators":          ["projects/library/validators/src/public-api.ts"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2015",
    "types": [
      "jest"
    ]
  }
}
